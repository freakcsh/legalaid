<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DialogConfirmBtnStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@drawable/bg_button_normal</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:paddingStart">20dp</item>
        <!--<item name="android:paddingTop">20dp</item>-->
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="DialogCancelBtnStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:background">@drawable/bg_button_gray</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:paddingStart">20dp</item>
        <!--<item name="android:paddingTop">8dp</item>-->
        <item name="android:textColor">@color/colorTextGray</item>
        <item name="android:textSize">15sp</item>
    </style>
</resources>